<h1>Login Admin</h1>

<%= form_with url: login_path, method: :post, local: true do %>
  <label>Login:</label>
  <%= text_field_tag :login, nil, required: true %><br><br>

  <label>Senha:</label>
  <%= password_field_tag :password, nil, required: true %><br><br>

  <%= submit_tag "Entrar" %>
<% end %>


